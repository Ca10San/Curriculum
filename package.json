{
  "name": "curriculum",
  "version": "1.0.0",
  "description": "curriculum made with just HTML and pure CSS",
  "main": "index.html",
  "scripts": {
    "commit": "npx git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ca10San/Curriculum.git"
  },
  "author": "Ca10San",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ca10San/Curriculum/issues"
  },
  "homepage": "https://github.com/Ca10San/Curriculum#readme",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "commitizen": "^4.1.2",
    "husky": "^4.2.5"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
